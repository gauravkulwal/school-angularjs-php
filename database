CREATE TABLE `school`.`class` ( `id` INT(255) NOT NULL AUTO_INCREMENT , `class` INT(255) NOT NULL , `rollnumber` VARCHAR(255) NOT NULL , `stname` VARCHAR(255) NOT NULL , `stfname` VARCHAR(255) NOT NULL , `address` VARCHAR(255) NOT NULL , `mobile` VARCHAR(255) NOT NULL , `image` BLOB NOT NULL , `stuser` VARCHAR(255) NOT NULL , `stpass` VARCHAR(255) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;